type User {       
  id: ID! @id             
  email: String! @unique  
  password: String!         
}

type Crime {
  id: ID! @id
  gu: String!  
  murder: Int!  
  robbery: Int!  
  rape: Int!  
  theft: Int! 
  violence: Int! 

}

type Station {
  id : ID! @id
  stationName : String!
  X : Float!
  Y : Float!
  stationAddress : String!

}