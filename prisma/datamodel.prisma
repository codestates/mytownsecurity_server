type User {       
  id: ID! @id             
  email: String! @unique  
  password: String!
  review: [Review]!
  favorite: [Favorite]!         
}

type Review {
  id : ID! @id
  text : String!
  grade : Float!
  postedBy : User
}

type Favorite {
  id : ID! @id
  placeAlias: String!
  postedBy : User
}

type Address {
  id: ID! @id
  detail: String!
  X : Float!
  Y : Float!
}

type Station {
  id : ID! @id
  stationName : String!
  X : Float!
  Y : Float!
  stationAddress : String!
}

type Crime {
  id: ID! @id
  gu: String!  
  murder: Int!  
  robbery: Int!  
  rape: Int!  
  theft: Int! 
  violence: Int! 
}