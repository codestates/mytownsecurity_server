type User {       
  id: ID! @id             
  email: String! @unique  
  password: String!
  review: [Review]!         
}

type Review {
  id : ID! @id
  text : String!
  grade : String!
  postedBy : User
}

type Crime {
  id: ID! @id
  gu: String!  
  murder: Int!  
  robbery: Int!  
  rape: Int!  
  theft: Int! 
  violence: Int! 
}

type Station {
  id : ID! @id
  stationName : String!
  X : Float!
  Y : Float!
  stationAddress : String!
}